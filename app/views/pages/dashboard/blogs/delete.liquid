---
slug: dashboard/blogs
method: delete
---
{% liquid
  function object = 'lib/data/queries/blogs/find', id: context.params.id
  function object = 'lib/commands/blogs/delete', object: object

  if object.valid
    include 'redirect_to', url: '/dashboard/blogs'
  else
    assign error = object.errors.base[0]

    function item_url = 'link_to', path: '/dashboard/blogs/', blogs: object
    include 'redirect_to', url: item_url, error: error
  endif
%}

