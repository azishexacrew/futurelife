<div class="slidershow-container">
  <div class="container">
      <div class="slidershow-content">
          <div id="myCarousel" class="carousel slide" data-interval="3000" data-ride="carousel">
              <!-- Carousel indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
      <div class="active item">
        <a href="#" title="banner1"><img class="lazy-image" data-original="" alt="banner1" src="{{ 'images/Futurelife-slideshow1.jpg' | asset_url }}" /></a>
      </div>
      <div class="item">
        <a href="#" title="banner2"><img class="lazy-image" data-original="" alt="banner2" src="{{ 'images/Futurelife-slideshow2.jpg' | asset_url }}" /></a>
      </div>
      <div class="item">
        <a href="#" title="banner2"><img class="lazy-image" data-original="" alt="banner2" src="{{ 'images/Futurelife-slideshow3.jpg' | asset_url }}" /></a>
      </div>
    </div>
          <a class="carousel-control right" href="#myCarousel" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-right"></span>
          </a>
          <a class="carousel-control left" href="#myCarousel" data-slide="next">
          <span class="glyphicon glyphicon-chevron-left"></span>
          </a>
          </div>
      </div>
      <!-- End slidershow-content -->
  </div>
</div>
<div class="container">
  <div class="title-text">
      <h3>Our new products</h3>
      <p>Lorem Ipsum is simply dummy text of the printing and typesetting industry.</p>
  </div>
  <!-- End title text -->
  <div class="slider-product our-new-product">
      <ul class="tabs">
          <li class="item" rel="tables">Tables</li>
          <li class="item" rel="chairs">Chairs</li>
          <li class="item" rel="lights">Lights</li>
          <li class="item" rel="sofas">Sofas</li>
          <li class="item" rel="accessories">Accessories</li>
      </ul>
      <div class="tab-container space-30">
        <div id="chairs" class="tab-content">
          <div class="product-tab-content products">
            {% for chair in itemsChairs.results %}
              <div class="item-inner">
                  <div class="product">
                    {% function item_url = 'link_to', path: '/items/', item: chair %}
                      <a class="product-images" href="{{ item_url }}" title="{{ chair.name }}">
                        {% if chair.photos.size > 1 %}
                          <img class="primary_image" src="{{ chair.photo.versions.uncropped }}" alt="{{ chair.name }}"/>
                          <img class="secondary_image" src="{{ chair.photo.versions.uncropped }}" alt="{{ chair.name }}"/>
                        {% else %}
                          {% function cover_url = 'theme/futurelife/dashboard/items/cover_url', item: chair %}
                          <img alt="{{ chair.name }}" class="primary_image" src="{{ cover_url }}">
                          <img alt="{{ chair.name }}" class="secondary_image" src="{{ cover_url }}">
                        {% endif %}
                      </a>
                      <p class="product-title">{{ chair.name }}</p>
                      <p class="product-price">{{ chair.price | pricify_cents }}</p>
                      <a class="add-to-cart" href="#">Add to Cart</a>
                      <div class="action">
                          <a href="#" title="Like"><i class="fa fa-heart-o"></i></a>
                          <a href="#" title="View"><i class="fa fa-compress"></i></a>
                      </div>
                  </div>
              </div>
            {% endfor %}
          </div>
          <!-- End product-tab-content products-->
      </div>
      <!-- End Chairs -->
      </div>
  </div>
  <!-- End OurNewProduct -->
  <div class="banner-center">
      <div class="col-md-3 first">
